{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run generate-types & vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "generate-types": "node scripts/generate-types.js",
    "pm2:start": "npm run generate-types && pm2 start node_modules/vite/bin/vite.js --name kb-final-frontend",
    "pm2:stop": "pm2 stop kb-final-frontend",
    "pm2:restart": "npm run generate-types && pm2 restart kb-final-frontend",
    "pm2:delete": "pm2 delete kb-final-frontend",
    "pm2:log": "pm2 logs",
    "pm2:status": "pm2 status",
    "pm2:which": "which pm2",
    "pm2:list": "pm2 list"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "pinia": "^3.0.3",
    "pm2": "^6.0.8",
    "swagger-typescript-api": "^13.2.7",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-datepicker-next": "^1.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10",
    "dotenv-cli": "^8.0.0",
    "cross-env": "^7.0.3"
  }
}
